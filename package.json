{
  "name": "podcast-api",
  "version": "1.0.0",
  "description": "Reproductor de audio con Cloudinary",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:api": "nodemon server.js",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "install:gh-pages": "npm install --save-dev gh-pages",
    "predeploy": "npm run build",
    "deploy-gh-pages": "gh-pages -d dist"
  },
  "dependencies": {
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "concurrently": "^9.1.2",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.1.9",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t0t/podcast_API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/t0t/podcast_API/issues"
  },
  "homepage": "https://github.com/t0t/podcast_API#readme"
}
